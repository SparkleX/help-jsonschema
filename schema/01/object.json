{
    "$schema": "http://json-schema.org/draft/2019-09/schema#",
    "title": "Example 01",
    "description": "",
	"type": "object",
	"additionalProperties": false,
	"properties": {
        "id": {
            "type": "number"
        },
		"name": {
            "type": "string",
            "default": "noname",
            "examples": ["aaa","bbb"]
        },
        "type": {
            "type": "string",
            "enum":["a","b"]
        },
        "datetime" : {
            "type": "string",
            "format" : "date-time"
        }
    },
    "required": ["id","name"]
}